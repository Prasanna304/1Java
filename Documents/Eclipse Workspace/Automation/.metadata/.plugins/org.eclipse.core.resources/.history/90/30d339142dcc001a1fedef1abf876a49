package polyTest;

public class ChildOverrideWithBothTest extends E_ParrentOverrideTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		ChildOverrideWithBothTest c = new ChildOverrideWithBothTest();
		E_ParrentOverrideTest d= new ChildOverrideWithBothTest();
		d.exe();
		c.exe();
	}
	public static void add() {
		System.out.println("child add");
	}
	public void exe() {
		add();
		super.add();
	}
}
