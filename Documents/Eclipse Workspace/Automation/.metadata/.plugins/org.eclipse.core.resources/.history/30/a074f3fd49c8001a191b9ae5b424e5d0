package a_SingleDimentionArray;

public class G_SoringArray {

	public static void main(String[] args) {
		
		int a[] = {1,5,3,6,8};
		int temp = 0;
		
		for (int i=0; i<a.length-1;i++) {
			for(int j=1; j<a.length;i++) {
				System.out.println(a[i]);
				System.out.println(a[j]);
				if(a[i]>a[j]) {
					a[i]=temp;
					temp=a[j];
					a[j]=a[i];		
				}
			}
				
			
			
		}

	}

}
